{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "outDir": "./dist",
    "sourceMap": true, // Necess√°rio para gerar arquivos de mapa de origem
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "types": ["node", "jest"]
  },
  "include": [
              "src/**/*"
, "config"            ],
  "exclude": ["node_modules"]
}
